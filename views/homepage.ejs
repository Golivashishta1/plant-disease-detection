        async function fetchWeatherData(location) {
        try {
            const weatherResponse = await fetch(`https://api.openweathermap.org/data/2.5/weather?q=${location}&appid=4b16cdce5f5106a911d7cb77652ef4df`);
            const weatherData = await weatherResponse.json();
            

            if (weatherData.main) {
                const temperature=Math.round(weatherData.main.temp - 273.15)
                document.getElementById("temperature-value").innerText = `${Math.round(weatherData.main.temp - 273.15)}°C`;
                document.getElementById("wind_speed_value").innerText = weatherData.wind.speed;
                document.getElementById("wind_humid_value").innerText = weatherData.main.humidity;
                
                let message = '';
            if (temperature < 10) {
                message = `In ${location}, temperatures are quite chilly, dipping below 10°C. During these colder months, it's crucial to protect your plants from frost damage. Consider using frost cloths or moving potted plants indoors. Ensure your plants are well-watered, but avoid over-watering, as the cold can slow down their growth.`;
            } else if (temperature >= 10 && temperature < 20) {
                message = `The current temperature in ${location} is mild, ranging from 10°C to 20°C. This is a great time to plant hardy varieties such as kale or pansies, which thrive in cooler weather. Be vigilant for any signs of fungal diseases, which can be more prevalent in humid conditions, and ensure proper airflow around your plants.`;
            } else if (temperature >= 20 && temperature < 30) {
                message = `With temperatures in ${location} hovering between 20°C and 30°C, it's the ideal weather for vibrant plant growth! This is a fantastic opportunity to water your plants regularly and consider applying a balanced fertilizer to boost their health. Keep an eye out for pests, as warmer weather can bring about an increase in insect activity.`;
            } else {
                message = `In the heat of ${location}, where temperatures exceed 30°C, it's essential to keep your plants hydrated. Water deeply in the early morning or late evening to prevent evaporation. Additionally, provide some shade for delicate plants to protect them from sunburn. Monitor for signs of heat stress, such as wilting or leaf scorch, and adjust your care accordingly.`;
            }

            // Display the message
            document.getElementById("temperature_message").innerText = message;
            } else {
                console.error("Weather data not found");
            }
        } catch (error) {
            console.error("Error fetching weather data:", error);
        }
    }
